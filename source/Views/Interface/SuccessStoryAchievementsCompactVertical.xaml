<UserControl x:Class="SuccessStory.Views.Interface.SuccessStoryAchievementsCompactVertical"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SuccessStory.Views.Interface"
             mc:Ignorable="d">

    <Border CornerRadius="{DynamicResource ControlCornerRadius}" BorderThickness="{DynamicResource ControlBorderThickness}" Background="Transparent" Padding="5"
            BorderBrush="{DynamicResource NormalBorderBrush}" MaxWidth="70">
        <Grid Name="PART_ScCompactView" Loaded="PART_ScCompactView_IsLoaded" 
              SizeChanged="PART_ScCompactView_SizeChanged"
              MinHeight="50" MaxWidth="50"></Grid>
    </Border>
</UserControl>
