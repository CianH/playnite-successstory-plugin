<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--
    THIS FILE IS NOT PART OF THE THEME, DO NOT MODIFY!
    -->

    <!-- StoreLocalization -->
    <sys:String x:Key="LOCLanguageNameEnglish">polish</sys:String>
    <sys:String x:Key="LOCLanguageCode">pl_PL</sys:String>
    <sys:String x:Key="LOCLanguageCountry">PL</sys:String>

    <!--  
    MainView
    -->
    <sys:String x:Key="LOCSucessStoryAchievements">Achievements</sys:String>

    <!--  
    SuccessStorySettingsView
    -->

    <sys:String x:Key="LOCSucessStoryEnableSteam">Enable Steam achievements.</sys:String>
    <sys:String x:Key="LOCSucessStoryEnableGog">Enable GOG achievements.</sys:String>
    <sys:String x:Key="LOCSucessStoryEnableOrigin">Enable Origin achievements.</sys:String>

    <sys:String x:Key="LOCSucessStoryRefreshData">Refresh data</sys:String>
    <sys:String x:Key="LOCSucessStoryRefreshAllDataRecent">Refresh all recent achievements data</sys:String>
    <sys:String x:Key="LOCSucessStoryRefreshAllData">Refresh all achievements data</sys:String>

    <sys:String x:Key="LOCSucessStoryRefreshDataMessage">Achievements database has been reset.</sys:String>

    <!-- 
    SuccessStoryView 
    -->

    <!-- UI -->
    <sys:String x:Key="LOCSucessStorySearch">Search</sys:String>
    <sys:String x:Key="LOCSucessStoryGlobalProgression">Global progression</sys:String>
    <sys:String x:Key="LOCSucessStoryGameInformation">Information</sys:String>

    <!-- ListViewGames -->
    <sys:String x:Key="LOCSucessStorylvGamesIcon">Icon</sys:String>
    <sys:String x:Key="LOCSucessStorylvGamesName">Name</sys:String>
    <sys:String x:Key="LOCSucessStorylvGamesLastActivity">Last session start</sys:String>
    <sys:String x:Key="LOCSucessStorylvGamesSourceName">Source</sys:String>
    <sys:String x:Key="LOCSucessStorylvGamesProgression">Progression</sys:String>

    <sys:String x:Key="LOCSucessStoryGraphicTitle">Achievements unllock by month</sys:String>
</ResourceDictionary>